{
  "name": "svelte-webpack",
  "version": "1.0.0",
  "description": "A simple Svelte app made using Webpack",
  "main": "index.js",
  "scripts": {
    "delete:build": "rimraf build",
    "prebuild": "npm run delete:build",
    "dev": "cross-env NODE_ENV=development webpack-dev-server",
    "build": "cross-env NODE_ENV=production webpack"
  },
  "keywords": [
    "svelte3",
    "routing",
    "webpack4"
  ],
  "author": "Ryan Dsouza",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-transform-classes": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "babel-loader": "^8.0.6",
    "core-js": "2.6.5",
    "critters-webpack-plugin": "^2.5.0",
    "cross-env": "^7.0.0",
    "css-loader": "^3.4.2",
    "dotenv-webpack": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.1",
    "lint-staged": "^10.0.7",
    "mini-css-extract-plugin": "^0.9.0",
    "preload-webpack-plugin": "^3.0.0-beta.4",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.1",
    "style-loader": "^1.1.3",
    "svelte": "^3.18.1",
    "svelte-loadable": "^1.4.0",
    "svelte-loader": "^2.13.6",
    "svelte-routing": "^1.4.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "dependencies": {}
}
